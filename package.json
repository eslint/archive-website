{
  "name": "eslint.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "This contains the code running on eslint.org.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build:espree": "browserify -x espree js/app/parser/index.js > js/app/parser/index.built.js",
    "build:eslint": "browserify --extension=.jsx --debug -x eslint -x espree -t babelify js/app/demo/main.js > js/app/demo.js",
    "build": "npm run build:espree && npm run build:eslint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eslint/eslint.github.io.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eslint/eslint.github.io/issues"
  },
  "browser": {
    "react": "./js/vendor/react.js",
    "reactDom": "./js/vendor/react_dom.js"
  },
  "homepage": "https://github.com/eslint/eslint.github.io#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^11.0.1",
    "json-loader": "^0.5.2",
    "node-libs-browser": "^0.5.2"
  }
}
