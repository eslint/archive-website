  },
  "homepage": "https://github.com/eslint/eslint.github.io#readme",
  "dependencies": {
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@octokit/rest": "^16.16.0",
    "bootstrap": "^3.4.1",
    "browserify": "^16.2.3",
    "eslint": "^4.5.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "json-loader": "^0.5.2",
